{
	"name": "test-ext",
	"displayName": "test_ext",
	"description": "testing",
	"version": "0.0.1",
	"enableProposedApi": true,
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:edgeworkers-vscode.edgeworkerBundle"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "edgeworkers-vscode.edgeworkerBundle",
				"title": "Create Edge worker Bundle"
			}
			
		],
		"menus": {
			"explorer/context": [{
				"when": "resourceFilename =~ /.*\\.(json)/",
				"command": "edgeworkers-vscode.edgeworkerBundle",
				"group": "bundle@1"
			}]
			
		},
		"configuration": {
			"title": "Akamai Configuration",
			"type": "object",
			"properties": {
				"edgeworkers-vscode.sectionName": {
					"type": "string",
					"default": "",
					"description": "section name lookup in .edgerc file"
				},
				"edgeworkers-vscode.accountKey": {
					"type": "string",
					"default": "",
					"description": "Account key only for internal users"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "^14.17.4",
		"@types/vscode": "^1.56.0",
		"@types/which": "^2.0.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
